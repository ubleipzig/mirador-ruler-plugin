{
  "name": "mirador-ruler",
  "version": "0.1.0",
  "description": "a pugon for mirador v3 that adds a ruler to the canvas view",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist package plugin.tar.gz",
    "build": "npm run clean && npx babel --presets=@babel/preset-react --plugins @babel/plugin-transform-modules-commonjs --out-dir dist src",
    "deploy": "webpack --config ./config/webpack.config.js --mode production",
    "package": "npm run build && mkdir package && cp -r package.json dist package && tar -cf plugin.tar.gz package",
    "start": "webpack-dev-server --mode development",
    "test": "node ./scripts/cypress.js"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.5.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@material-ui/core": "^4.1.3",
    "@material-ui/icons": "^4.2.1",
    "babel-loader": "^8.0.5",
    "css-loader": "^3.1.0",
    "cypress": "^3.4.0",
    "html-webpack-plugin": "^3.2.0",
    "mirador": "^3.0.0-alpha.16",
    "npm-install-peers": "^1.2.1",
    "npm-run-all": "^4.1.5",
    "react": "^16.8.6",
    "react-dev-utils": "^9.0.1",
    "react-redux": "^7.1.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.35.2",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "prop-types": "^15.7.2",
    "reselect": "^4.0.0"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.1.3",
    "@material-ui/icons": "^4.2.1",
    "mirador": "^3.0.0-alpha.16",
    "react": "^16.8.3",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0"
  }
}
